{
  "name": "armandoblog",
  "version": "1.0.0",
  "description": "A blog where you can post anything you would like.",
  "main": "app.js",
  "scripts": {
    "test": "\"no test command\"",
    "pretest": "eslint --ignore-path .gitignore .",
    "start": "DEBUG=app,app:* nodemon app.js",
    "dev": "webpack-dev-server --config webpack.config.dev.js --port 3000"
  },
  "keywords": [
    "blog",
    "armando",
    "ejs",
    "fullstack"
  ],
  "author": "Armando Salinas",
  "license": "MIT",
  "dependencies": {
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "chalk": "^4.1.0",
    "css-loader": "^4.2.1",
    "debug": "^4.1.1",
    "eslint": "^7.3.1",
    "eslint-loader": "^4.0.2",
    "html-loader": "^1.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "1000",
    "env": {
      "NODE_ENV": "development",
      "PORT": 4000
    }
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  }
}
